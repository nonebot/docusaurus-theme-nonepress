{
  "name": "@nullbot/docusaurus-theme-nonepress",
  "version": "3.0.11",
  "description": "Nonepress theme for Docusaurus",
  "main": "lib/index.js",
  "types": "src/types.d.ts",
  "exports": {
    "./lib/*": "./lib/*",
    "./src/*": "./src/*",
    "./client": {
      "type": "./lib/client/index.d.ts",
      "default": "./lib/client/index.js"
    },
    "./contexts": {
      "type": "./lib/contexts/index.d.ts",
      "default": "./lib/contexts/index.js"
    },
    ".": {
      "types": "./src/types.d.ts",
      "default": "./lib/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nonebot/docusaurus-theme-nonepress.git",
    "directory": "packages/theme-nonepress"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc --build && node ../../scripts/copyFiles.js && prettier --config ../../.prettierrc --write \"lib/theme/**/*.js\"",
    "watch": "run-p -c copy:watch build:watch",
    "build:watch": "tsc --build --watch",
    "copy:watch": "node ../../scripts/copyFiles.js --watch"
  },
  "dependencies": {
    "@docsearch/react": "3.5.2",
    "@docusaurus/core": "3.5.2",
    "@docusaurus/mdx-loader": "3.5.2",
    "@docusaurus/module-type-aliases": "3.5.2",
    "@docusaurus/plugin-content-blog": "3.5.2",
    "@docusaurus/plugin-content-docs": "3.5.2",
    "@docusaurus/plugin-content-pages": "3.5.2",
    "@docusaurus/theme-common": "3.5.2",
    "@docusaurus/theme-translations": "3.5.2",
    "@docusaurus/types": "3.5.2",
    "@docusaurus/utils": "3.5.2",
    "@docusaurus/utils-common": "3.5.2",
    "@docusaurus/utils-validation": "3.5.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mdx-js/react": "^3.0.0",
    "@nullbot/docusaurus-plugin-docsmenu": "^3.0.11",
    "@nullbot/docusaurus-plugin-getsidebar": "^3.0.11",
    "@tailwindcss/typography": "^0.5.0",
    "algoliasearch": "^4.18.0",
    "algoliasearch-helper": "^3.13.3",
    "autoprefixer": "^10.3.1",
    "clsx": "^2.0.0",
    "copy-text-to-clipboard": "^3.2.0",
    "daisyui": "^4.0.0",
    "infima": "0.2.0-alpha.43",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.26",
    "postcss-import": "^15.1.0",
    "prism-react-renderer": "^2.3.0",
    "prismjs": "^1.29.0",
    "react-router-dom": "^5.3.4",
    "rtlcss": "^4.1.0",
    "tailwindcss": "^3.0.0",
    "tslib": "^2.6.0",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@types/nprogress": "^0.2.0",
    "@types/prismjs": "^1.26.0",
    "@types/rtlcss": "^3.5.1",
    "fs-extra": "^11.1.1",
    "react-test-renderer": "^18.0.0",
    "utility-types": "^3.10.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "engines": {
    "node": ">=18.0"
  }
}
